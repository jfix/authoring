<xs:schema xmlns="urn:xpressauthor:xpressschema" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:xpressauthor:xpressschema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="XpressSchema">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Attributes" maxOccurs="unbounded"/>
				<xs:element ref="Emphasis" maxOccurs="unbounded"/>
				<xs:element ref="Elements" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="lineType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="Invalid"/>
			<xs:enumeration value="None"/>
			<xs:enumeration value="Single"/>
			<xs:enumeration value="Dot"/>
			<xs:enumeration value="DashSmallGap"/>
			<xs:enumeration value="DashLargeGap"/>
			<xs:enumeration value="DashDot"/>
			<xs:enumeration value="DashDotDot"/>
			<xs:enumeration value="Double"/>
			<xs:enumeration value="Triple"/>
			<xs:enumeration value="ThinThickSmallGap"/>
			<xs:enumeration value="ThickThinSmallGap"/>
			<xs:enumeration value="ThinThickThinSmallGap"/>
			<xs:enumeration value="ThinThickMedGap"/>
			<xs:enumeration value="ThickThinMedGap"/>
			<xs:enumeration value="ThinThickThinMedGap"/>
			<xs:enumeration value="ThinThickLargeGap"/>
			<xs:enumeration value="ThickThinLargeGap"/>
			<xs:enumeration value="ThinThickThinLargeGap"/>
			<xs:enumeration value="SingleWavy"/>
			<xs:enumeration value="DoubleWavy"/>
			<xs:enumeration value="DashDotStroked"/>
			<xs:enumeration value="Emboss3D"/>
			<xs:enumeration value="Engrave3D"/>
			<xs:enumeration value="Outset"/>
			<xs:enumeration value="Inset"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="allowDeny">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="allow"/>
			<xs:enumeration value="deny"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:attributeGroup name="commonAttributes">
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="namespace" type="xs:string" use="optional"/>
		<xs:attribute name="prefix" type="xs:string" use="optional"/>
		<xs:attribute name="xmlname" type="xs:string" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="fieldAttributes">
		<xs:attribute name="datatype" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="string"/>
					<xs:enumeration value="multiline"/>
					<xs:enumeration value="datetime"/>
					<xs:enumeration value="time"/>
					<xs:enumeration value="uniqueidentifier"/>
					<xs:enumeration value="droplist"/>
					<xs:enumeration value="combobox"/>
          <xs:enumeration value="imagewidth"/>
          <xs:enumeration value="imageheight"/>
          <xs:enumeration value="imagewidthdpi"/>
          <xs:enumeration value="imageheightdpi"/>
					<xs:enumeration value="sectionBreak"/>
					<xs:enumeration value="pageBreak"/>
					<xs:enumeration value="columnBreak"/>
					<xs:enumeration value="tableBorderBottomStyle"/>
					<xs:enumeration value="tableBorderRightStyle"/>
					<xs:enumeration value="tableFrame"/>
					<xs:enumeration value="tableRowColumnIndex"/>
					<xs:enumeration value="tableRowColumnIdentifier"/>
					<xs:enumeration value="tableColumnWidth"/>
					<xs:enumeration value="tableRowHeight"/>
					<xs:enumeration value="tableCellColumnStart"/>
					<xs:enumeration value="tableCellColumnEnd"/>
					<xs:enumeration value="tableCellRowCount"/>
					<xs:enumeration value="tableCellColumnCount"/>
					<xs:enumeration value="tableCellContentRotate"/>
					<xs:enumeration value="tableAlignmentHorizontal"/>
					<xs:enumeration value="tableAlignmentVertical"/>
					<xs:enumeration value="tableStyle"/>
					<xs:enumeration value="tableColumnCount"/>
					<xs:enumeration value="tableRowCount"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="format" type="xs:string" use="optional"/>
		<xs:attribute name="pattern" type="xs:string" use="optional"/>
		<xs:attribute name="displayPattern" type="xs:string" use="optional"/>
		<xs:attribute name="friendly" type="xs:string" use="optional"/>
		<xs:attribute name="attrGroupName" type="xs:string" use="optional"/>
		<xs:attribute name="default" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="fixed" type="xs:string" use="optional"/>
		<xs:attribute name="visible" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="readonly" type="xs:boolean" use="optional" default="true"/>
		<xs:attribute name="required" type="xs:boolean" use="optional" default="true"/>
		<xs:attribute name="externalMethodId" type="xs:string" use="optional"/>
		<xs:attribute name="keepWithNext" type="xs:boolean"/>
		<xs:attribute name="widowControl" type="xs:boolean"/>
	</xs:attributeGroup>
