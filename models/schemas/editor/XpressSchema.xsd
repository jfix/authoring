	<xs:attributeGroup name="tableStyles">
		<xs:attribute name="horizAlign" use="optional" default="left">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="left"/>
					<xs:enumeration value="right"/>
					<xs:enumeration value="center"/>
					<xs:enumeration value="justify"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="vertAlign" use="optional" default="top">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="top"/>
					<xs:enumeration value="middle"/>
					<xs:enumeration value="bottom"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="backColor" type="xs:string" use="optional"/>
		<xs:attribute name="frame" type="xs:NMTOKEN" use="optional" default="all"/>
		<xs:attribute name="frameStyle" type="xs:NMTOKEN" use="optional" default="single"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="tgroupConstraint">
		<xs:attribute name="insertDeleteColumns" type="allowDeny" use="optional" default="allow">
			<xs:annotation>
				<xs:documentation>Controls the ability to insert and delete columns within this group</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="changeWidth" type="allowDeny" use="optional" default="allow">
			<xs:annotation>
				<xs:documentation>Controls modifying the width of columns within this group </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="sectionConstraint">
		<xs:attribute name="insertDeleteRows" type="allowDeny" use="optional" default="allow">
			<xs:annotation>
				<xs:documentation>Controls the ability to insert and delete rows within this section</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="changeHeight" type="allowDeny" use="optional" default="allow">
			<xs:annotation>
				<xs:documentation>Controls modifying the height of rows within this section</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="changeBorders" type="allowDeny" use="optional" default="allow">
			<xs:annotation>
				<xs:documentation>Controls modifying the border state of cells within this section</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="splitCell" type="allowDeny" use="optional" default="allow">
			<xs:annotation>
				<xs:documentation>Controls the ability to split cells within this section</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="mergeCells" type="allowDeny" use="optional" default="allow">
			<xs:annotation>
				<xs:documentation>Controls the ability to merge cells within this section</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="xpressColumnStyleAttributes">
		<xs:attribute name="columnBorderStyle" type="lineType" use="optional" default="Single"/>
		<xs:attribute name="columnBorderColor" type="rgbType" use="optional" default="000000"/>
		<xs:attribute name="columnBorderWidth" type="xs:string" use="optional" default="1px"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="xpressRowStyleAttributes">
		<xs:attribute name="rowBorderStyle" type="lineType" use="optional" default="Single"/>
		<xs:attribute name="rowBorderColor" type="rgbType" use="optional" default="000000"/>
		<xs:attribute name="rowBorderWidth" type="xs:string" use="optional" default="1px"/>
	</xs:attributeGroup>
	<xs:element name="ExtensibilityMethods">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ExtensibilityMethod" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attribute name="id" type="xs:string" use="required"/>
						<xs:attribute name="friendly" type="xs:string" use="optional"/>
						<xs:attribute name="showInComponentContextMenu" type="xs:boolean" use="optional"/>
						<xs:attribute name="showInContextMenu" type="xs:boolean" use="optional"/>
						<xs:attribute name="showXPath" type="xs:string" use="optional"/>
						<xs:attribute name="enableXPath" type="xs:string" use="optional"/>
						<xs:attribute name="faceID" type="xs:string" use="optional"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Attributes">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="AttributeDef" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Restriction" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Enumeration" maxOccurs="unbounded">
											<xs:complexType>
												<xs:attribute name="value" type="xs:string" use="required"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attributeGroup ref="commonAttributes"/>
						<xs:attributeGroup ref="fieldAttributes"/>
						<xs:attribute name="contextId" type="xs:string" use="optional"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Emphasis">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="EmphasisDef" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Specialized" minOccurs="0">
								<xs:complexType>
									<xs:choice>
										<xs:element name="Footnote">
											<xs:annotation>
												<xs:documentation>This emphasis is a Word footnote/endnote emphasis </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="footnoteLocation" use="required">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="text"/>
															<xs:enumeration value="section"/>
															<xs:enumeration value="page"/>
															<xs:enumeration value="document"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="allowedNumberingSchemes" type="xs:string" default="arabic bigAlpha smallAlpha bigRoman smallRoman">
													<xs:annotation>
														<xs:documentation>When ommitted, the list is:  arabic bigAlpha smallAlpha bigRoman smallRoman.  The first token specified is the default scheme.</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="captionAttribute" type="xs:string" use="optional">
													<xs:annotation>
														<xs:documentation>Specifies a child attribute to contain the character used for the footnote link, for example a number or an alpha character.</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="disableCustomMarkTextbox" type="xs:boolean" use="optional" default="false"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="Endnote">
											<xs:annotation>
												<xs:documentation>This emphasis is a Xpress Endnote</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="endnoteElementDef" type="xs:string" use="required">
													<xs:annotation>
														<xs:documentation>Specifies the ElementDef name that is to contain this endnote item.</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="allowedNumberingSchemes" type="xs:string" default="arabic bigAlpha smallAlpha bigRoman smallRoman">
													<xs:annotation>
														<xs:documentation>When ommitted, the list is:  arabic bigAlpha smallAlpha bigRoman smallRoman.  The first token specified is the default scheme.</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="captionAttribute" type="xs:string">
													<xs:annotation>
														<xs:documentation>Specifies a child attribute to contain the character used for the endnote link, for example a number or an alpha character.</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="disableCustomMarkTextbox" type="xs:boolean" use="optional" default="false"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="Link">
											<xs:annotation>
												<xs:documentation>This emphasis is a hyperlink emphasis that can be of 3 types-Internal/External/Both</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:choice>
													<xs:element ref="External"/>
													<xs:element ref="Multiple"/>
													<xs:choice>
														<xs:element ref="Internal"/>
														<xs:element ref="Custom"/>
													</xs:choice>
												</xs:choice>
											</xs:complexType>
										</xs:element>
										<xs:element name="Entity">
											<xs:annotation>
												<xs:documentation>This emphasis type is used to tag an XML Entity in the document. Any content entity that is tagged by this emphasis will maintian a synchronous mapping with the entity node in the document.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:choice>
													<xs:element name="Wrapper">
														<xs:annotation>
															<xs:documentation>This is used for backward compatability to allow for metadata-based (attributes) entity support. With this approach, an entity can be associated with metadata which will persist in the serialized document. This metadata among other things can be used to control whether an enity can be edited/deleted (using attributeName/attributeValuepairs ) from the Modify/Delete children </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence minOccurs="0">
																<xs:element name="Add" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Specifies whether an entity can be added thru the UI</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:attribute name="allow" type="xs:boolean" use="required"/>
																	</xs:complexType>
																</xs:element>
																<xs:element name="Modify" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>If allow=true, or an entity reference wrapper element has attributeName = attributeValue, the instance value may be edited through the UI.</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:attribute name="attributeName" type="xs:string" use="optional"/>
																		<xs:attribute name="attributeValue" type="xs:string" use="optional"/>
																		<xs:attribute name="allow" type="xs:boolean" use="optional"/>
																	</xs:complexType>
																</xs:element>
																<xs:element name="Delete" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>If an entity reference wrapper element has attributeName = attributeValue, the instance may be deleted through the UI.  If allow = true, the entity definition may be deleted, provided there are no entity references.</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:attribute name="attributeName" type="xs:string" use="optional"/>
																		<xs:attribute name="attributeValue" type="xs:string" use="optional"/>
																		<xs:attribute name="allow" type="xs:boolean" use="optional"/>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="entityNameAttribute" use="required"/>
														</xs:complexType>
													</xs:element>
													<xs:element name="UserDefined">
														<xs:annotation>
															<xs:documentation>This is used to support a more genric entity scenario where metadata and wrapper emphasis are not preserved.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="allowAdd" type="xs:boolean">
																<xs:annotation>
																	<xs:documentation>Specifies whether a new Internal entity definition can be added to the current document.</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="allowModify" type="xs:boolean">
																<xs:annotation>
																	<xs:documentation>Specifies whether an entity value can be modified. This attribute is not relevant for external entities - XA will not modify external entity definitions.</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="allowDelete" type="xs:boolean">
																<xs:annotation>
																	<xs:documentation>Specifies whether an entity definition can de deleted. An entity definition can de deleted only if there is no entity references within the document. This attribute is irrelevant for external entities - XA will not delete external entity definitions.</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
												</xs:choice>
											</xs:complexType>
										</xs:element>
										<xs:element name="ProcessingInstruction"/>
									</xs:choice>
								</xs:complexType>
							</xs:element>
							<xs:sequence>
								<xs:element name="Style">
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="StyleType"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="SubStyles" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="SubStyle" maxOccurs="unbounded">
												<xs:complexType>
													<xs:sequence>
														<xs:element name="Style" type="StyleType"/>
													</xs:sequence>
													<xs:attribute name="value" type="xs:string" use="required"/>
													<xs:attribute name="friendly" type="xs:string" use="optional"/>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
										<xs:attribute name="key" type="xs:string" use="required"/>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:element name="Attributes" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="AttributeType" maxOccurs="unbounded">
											<xs:complexType>
												<xs:attribute name="name" type="xs:string" use="required"/>
												<xs:attributeGroup ref="fieldAttributes"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element ref="ExtensibilityMethods" minOccurs="0"/>
							<xs:element name="Emphasis" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="EmphasisType" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:attribute name="name" type="xs:string" use="required"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element ref="Media" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Defines any inline image elements (both reference and embedded) for the parent (para or emphasis)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="OLE" minOccurs="0"/>
						</xs:sequence>
						<xs:attributeGroup ref="commonAttributes"/>
						<xs:attribute name="friendly" type="xs:string" use="optional"/>
						<xs:attribute name="readonly" type="xs:boolean" use="optional"/>
						<xs:attribute name="excludeFromEmphasisCombo" type="xs:boolean" use="optional"/>
						<xs:attribute name="tooltipTransform" type="xs:string" use="optional"/>
						<xs:attribute name="selectOnFocus" type="xs:boolean" use="optional" default="false"/>
						<xs:attribute name="contextId" type="xs:string" use="optional"/>
						<xs:attribute name="preserveSpace" type="xs:boolean" use="optional" default="false"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Elements">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ElementDef" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:choice>
								<xs:element name="OLE">
									<xs:annotation>
										<xs:documentation>Defines an OLE element of type indicated in classType attribute.</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="MathTypeEquation">
												<xs:annotation>
													<xs:documentation>Defines MathType equation.													</xs:documentation>
