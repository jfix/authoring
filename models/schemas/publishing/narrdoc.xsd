    </xs:attribute>
    <xs:attribute name="reference"/>
    <xs:attribute name="dynamic-graph">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="y"/>
          <xs:enumeration value="n"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for tableheading element : ??
    
    container used to store the heading of a table.
  -->
  <xs:element name="tableheading">
    <xs:complexType>
