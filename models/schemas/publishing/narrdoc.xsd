        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="hyperlink"/>
          <xs:element ref="fignote"/>
          <xs:element ref="source"/>
          <xs:element ref="legend"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.figure"/>
    </xs:complexType>
