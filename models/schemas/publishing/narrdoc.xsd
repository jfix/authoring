<?xml version="1.0" encoding="UTF-8"?>
<!--
  @title narrdoc v1.7.3
  @doctype PUBLIC "-//OECD//DTD Narrdoc 1.7 light//EN" "P:/Production/Software/Compo/Dev/Converter/word2xmlsvn/Tests/model/narrdoc1_7light.dtd"
  @hidden Author: 
     Marc Regnier
     Simplified DTD of the 1.5 version (used by OECD/PrePress for typeset publications).
     Used with XML files from the output of the UpCast Converter.
     Version: 1.7.3
     History:
     V0.1: 28.02.2011
     Add attribute ROWSEP in ROW element
     Delete COTE element in BIBINFO
     Change element BODY optional in NARR-DOC element
     V0.2: 29.06.2011
     Re-order the elements in <frontmatter> and <name>
     Persmission for a <shortcitation> in a <shortcitation>
     jfix: added the attribute @metadatatype@ for the part, chapter and 
     section elements (see #2698), for the metadata loader program (again!)
     only one possible value at this time: @section@.
     V0.3: 07.02.2012
     jfix: added a parameter entity for a geocontainer attribute
     see #2494 and #2449, requested for metadata loader
     rcouronne: added a parameter entity for a statisticalannex attribute
     see #2903, requested for metadata loader
     Add the attributes reference, goecontainer and statisticalannex in annex and appendix elements
     Change the numbering value from "n" to "y" in annex and appendix elements
     Add reference attribute in boxsection element
     Add the attributes reference, goecontainer, statisticalannex and metadatatype in chapter element
     Change the numbering value from "n" to "y" in chapter element
     Allow shortcitation element in emphasis element
     Add the attribute reference in figure element
     Re-order the frontmatter element to allow any element in any order
     Re-order name element
     Re-order nar-rdoc element
     Change the content model in oecd-doc element
     Add the attributes reference, goecontainer, statisticalannex and metadatatype in part element
     Change the numbering value from "n" to "y" in part element
     Add rowsep attribute in row element
     Allow shortcitation element in shortcitation element
     Add reference attribute in tablegrp element
     Add reference attribute in tgroup element
     V0.4: 22.02.12
     Changed element frontmatter: now element authorlist may only occur directly after bibinfo
  @root narr-doc
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  
  <xs:annotation xmlns:ctrl="http://nwalsh.com/xmlns/schema-control/"
    xmlns:xd="http://www.oecd.org/ns/schema/xdocs"
    xmlns:db="http://docbook.org/ns/docbook"
    xmlns:s="http://www.ascc.net/xml/schematron"
    xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:svg="http://www.w3.org/2000/svg"
    xmlns="http://www.oecd.org/ns/model"
    xmlns:rng="http://relaxng.org/ns/structure/1.0"
    xmlns:mml="http://www.w3.org/1998/Math/MathML"
    xmlns:html="http://www.w3.org/1999/xhtml">
    <xs:appinfo>
      <xd:name>narrdoc</xd:name>
      <xd:root>narr-doc</xd:root>
    </xs:appinfo>
    <xs:documentation>
      <xd:p>Narrdoc is Document Type Definition used by OECD Prepress depertment for typeset publications)
        
        the components used in narrative
        models for capturing official OECD documents and publications. To date,
        "pars" supports all documents and camera-ready and typset
        publications.</xd:p>
    </xs:documentation>
  </xs:annotation>
  
  <!-- 
    
    http://publications.oecd.org/documentation/Projects/ID0191/MOSCA4/Nomenclature.xml
  
  -->
  
  
  <xs:attributeGroup name="metadatatype">
    <xs:attribute name="metadatatype">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="section"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  
  <xs:attributeGroup name="id">
    <xs:attribute name="id" use="required" type="xs:ID"/>
  </xs:attributeGroup>
  
  <xs:attributeGroup name="instlang">
    <xs:attribute name="instlang">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="i-eng"/>
          <xs:enumeration value="i-fr"/>
          <xs:enumeration value="i-bi-ling"/>
          <xs:enumeration value="i-de"/>
          <xs:enumeration value="i-other"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  
  <xs:attributeGroup name="geocontainer">
    <xs:attribute name="geocontainer" default="false">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  
  <xs:attributeGroup name="statisticalannex">
    <xs:attribute name="statisticalannex" default="false">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for abbrtitl element : ??
    
    Container used to store the abbreviated title (text only). 
  -->
  <xs:element name="abbrtitl" type="xs:string"/>
  <!-- Element not used -->
  <xs:element name="about">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="heading"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="subheading"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="authorlist"/>
        <xs:element minOccurs="0" ref="highlight"/>
        <xs:element minOccurs="0" ref="abstract"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:choice maxOccurs="unbounded">
            <xs:element ref="p"/>
            <xs:element ref="citation"/>
            <xs:element ref="deflist"/>
            <xs:element ref="randlist"/>
            <xs:element ref="seqlist"/>
          </xs:choice>
          <xs:element ref="figure"/>
          <xs:element ref="tablegrp"/>
          <xs:element ref="textbox"/>
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="section"/>
        <xs:element minOccurs="0" ref="chapconcl"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.about"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.about">
    <xs:attributeGroup ref="id"/>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for abstract element : ??
        NIC Model equivalent for id attribute : ??
        NIC Model equivalent for instlang attribute : ??
    
    Container used to store the text converted from the Word style abstract. The text is a short description introducing the parent element listed above.  
  -->
  <xs:element name="abstract">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="p"/>
          <xs:element ref="citation"/>
          <xs:element ref="deflist"/>
          <xs:element ref="randlist"/>
          <xs:element ref="seqlist"/>
        </xs:choice>
        <xs:element ref="figure"/>
        <xs:element ref="tablegrp"/>
        <xs:element ref="textbox"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.abstract"/>
    </xs:complexType>
  </xs:element>
  <!--
    @attr instlang define the document language. It can have the following values:
    <ul><li>i-eng for english</li>
    <li>i-fr for french</li>
    <li>i-de for deutsch</li>
    <li>i-bi-ling for bilingual</li>
    <li>i-other for other language</li></ul>
  -->
  <xs:attributeGroup name="attlist.abstract">
    <xs:attributeGroup ref="id"/>
    <xs:attributeGroup ref="instlang"/>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for acknowledgement element : ??
        NIC Model equivalent for id attribute : ??
    
    Container used to begin a acknoledgement section. It store the text converted from the Word style Acknowledgement Heading.<br>
    acknowledgement element can be used in the following sections:
        <ul><li>frontmatter</li>
        <li>part</li></ul>
  -->
  <xs:element name="acknowledgement">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="heading"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="subheading"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="authorlist"/>
        <xs:element minOccurs="0" ref="highlight"/>
        <xs:element minOccurs="0" ref="abstract"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:choice maxOccurs="unbounded">
            <xs:element ref="p"/>
            <xs:element ref="citation"/>
            <xs:element ref="deflist"/>
            <xs:element ref="randlist"/>
            <xs:element ref="seqlist"/>
          </xs:choice>
          <xs:element ref="figure"/>
          <xs:element ref="tablegrp"/>
          <xs:element ref="textbox"/>
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="section"/>
        <xs:element minOccurs="0" ref="chapconcl"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.acknowledgement"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.acknowledgement">
    <xs:attributeGroup ref="id"/>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for acronym element : ??
        NIC Model equivalent for id attribute : ??
    
    Container used to store the text of an acronym.
    acronym element is used in glossary-acronyms section.
    Could contain text and special character (subscript/superscript for exemple).
  -->
  <xs:element name="acronym">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="specchar"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.acronym"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.acronym">
    <xs:attributeGroup ref="id"/>
  </xs:attributeGroup>
  <!-- Element not used -->
  <xs:element name="action">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="p"/>
          <xs:element ref="citation"/>
          <xs:element ref="deflist"/>
          <xs:element ref="randlist"/>
          <xs:element ref="seqlist"/>
        </xs:choice>
        <xs:element ref="figure"/>
        <xs:element ref="tablegrp"/>
        <xs:element ref="textbox"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.action"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.action">
    <xs:attributeGroup ref="id"/>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for annex element : ??
        NIC Model equivalent for id attribute : ??
        NIC Model equivalent for label attribute : ??
        NIC Model equivalent for number attribute : ??
        NIC Model equivalent for numbering attribute : ??
    
    Container used to begin a annex section. It store the text converted from the Word style Annex Heading.<br>
        annex element can be used in the following sections:
        <ul><li>backtmatter</li>
        <li>chapter</li>
        <li>part</li></ul>
  -->
  <xs:element name="annex">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="heading"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="subheading"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="authorlist"/>
        <xs:element minOccurs="0" ref="highlight"/>
        <xs:element minOccurs="0" ref="abstract"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:choice maxOccurs="unbounded">
            <xs:element ref="p"/>
            <xs:element ref="citation"/>
            <xs:element ref="deflist"/>
            <xs:element ref="randlist"/>
            <xs:element ref="seqlist"/>
          </xs:choice>
          <xs:element ref="figure"/>
          <xs:element ref="tablegrp"/>
          <xs:element ref="textbox"/>
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="section"/>
        <xs:element minOccurs="0" ref="chapconcl"/>
        <xs:element minOccurs="0" ref="biblio"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.annex"/>
    </xs:complexType>
  </xs:element>
  <!--
    @attr label is used to store the numbering to be displayed.
    @attr number is used to store the numbering to be displayed.
    @attr numbering is used to activate the numbering. Possible values are <i>y</i> (yes) or <i>n</i> (no).
    @attr reference is used to store the numbering to be displayed.
    @attr geocontainer is requested for metadata loader. Possible values are <i>false</i> or <i>true</i>.
    @attr statisticalannex is requested for metadata loader. Possible values are <i>false</i> or <i>true</i>.
  -->
  <xs:attributeGroup name="attlist.annex">
    <xs:attributeGroup ref="id"/>
    <xs:attribute name="label"/>
    <xs:attribute name="number"/>
    <xs:attribute name="numbering" default="y">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="y"/>
          <xs:enumeration value="n"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="reference"/>
    <xs:attributeGroup ref="geocontainer"/>
    <xs:attributeGroup ref="statisticalannex"/>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for appendix element : ??
        NIC Model equivalent for id attribute : ??
        NIC Model equivalent for label attribute : ??
        NIC Model equivalent for number attribute : ??
        NIC Model equivalent for numbering attribute : ??
    
    Container used to begin a appendix section. It store the text converted from the Word style Appendix Heading.<br>
        appendix element can be used in the following sections:
        <ul><li>backtmatter</li>
        <li>chapter</li>
        <li>part</li></ul>
  -->
  <xs:element name="appendix">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="heading"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="subheading"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="authorlist"/>
        <xs:element minOccurs="0" ref="highlight"/>
        <xs:element minOccurs="0" ref="abstract"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:choice maxOccurs="unbounded">
            <xs:element ref="p"/>
            <xs:element ref="citation"/>
            <xs:element ref="deflist"/>
            <xs:element ref="randlist"/>
            <xs:element ref="seqlist"/>
          </xs:choice>
          <xs:element ref="figure"/>
          <xs:element ref="tablegrp"/>
          <xs:element ref="textbox"/>
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="section"/>
        <xs:element minOccurs="0" ref="chapconcl"/>
        <xs:element minOccurs="0" ref="biblio"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.appendix"/>
    </xs:complexType>
  </xs:element>
  <!--
    @attr label is used to store the numbering to be displayed.
    @attr number is used to store the numbering to be displayed.
    @attr numbering is used to activate the numbering. Possible values are <i>y</i> (yes) or <i>n</i> (no).
    @attr reference is used to store the numbering to be displayed.
    @attr geocontainer is requested for metadata loader. Possible values are <i>false</i> or <i>true</i>.
    @attr statisticalannex is requested for metadata loader. Possible values are <i>false</i> or <i>true</i>.
  -->
  <xs:attributeGroup name="attlist.appendix">
    <xs:attributeGroup ref="id"/>
    <xs:attribute name="label"/>
    <xs:attribute name="number"/>
    <xs:attribute name="numbering" default="y">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="y"/>
          <xs:enumeration value="n"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="reference"/>
    <xs:attributeGroup ref="geocontainer"/>
    <xs:attributeGroup ref="statisticalannex"/>
  </xs:attributeGroup>
  <!-- Element not used -->
  <xs:element name="article">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="name"/>
        <xs:element ref="arttitle"/>
        <xs:element minOccurs="0" ref="editor"/>
        <xs:element ref="hosttitl"/>
        <xs:element minOccurs="0" ref="location"/>
        <xs:element minOccurs="0" ref="publishr"/>
        <xs:element ref="date"/>
        <xs:element minOccurs="0" ref="work-id"/>
        <xs:element minOccurs="0" ref="volno"/>
        <xs:element minOccurs="0" ref="issueno"/>
        <xs:element minOccurs="0" ref="pgnos"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.article"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.article">
    <xs:attributeGroup ref="id"/>
  </xs:attributeGroup>
  <!-- Element not used -->
  <xs:element name="arttitle" type="xs:string"/>
  <!--
        NIC Model equivalent for author element : ??
    
    Container used to store the data of an author as following:<br>
    <ul><li>name</li>
    <li>position</li>
    <li>telephone</li>
    <li>email</li>
    <li>fax</li>
  -->
  <xs:element name="author">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="name"/>
        <xs:element minOccurs="0" ref="position"/>
        <xs:element minOccurs="0" ref="telephone"/>
        <xs:element minOccurs="0" ref="email"/>
        <xs:element minOccurs="0" ref="fax"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
        NIC Model equivalent for authorlist element : ??
    
    Container introducing an author element. Converted from the Word style Author.
  -->
  <xs:element name="authorlist">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="author"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
        NIC Model equivalent for backmatter element : ??
        NIC Model equivalent for instlang attribute : ??
    
    The backmatter element is used to store all the elements at the end of a document, after the last part or chapter. For exemple, an annex in the backmatter is refering to the entire document. It can contains:<br>
    <ul><li>conclusion</li>
    <li>glossary-acronyms</li>
    <li>biblio</li>
    <li>appendix</li>
    <li>annex</li>
    <li>info</li></ul>
  -->
  <xs:element name="backmatter">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="conclusion"/>
        <xs:element ref="glossary-acronyms"/>
        <xs:element ref="biblio"/>
        <xs:element ref="appendix"/>
        <xs:element maxOccurs="unbounded" ref="annex"/>
        <xs:element ref="info"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.backmatter"/>
    </xs:complexType>
  </xs:element>
  <!--
    @attr instlang define the document language. It can have the following values:
    <ul><li>i-eng for english</li>
    <li>i-fr for french</li>
    <li>i-de for deutsch</li>
    <li>i-bi-ling for bilingual</li>
    <li>i-other for other language</li></ul>
  -->
  <xs:attributeGroup name="attlist.backmatter">
    <xs:attributeGroup ref="instlang"/>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for bibinfo element : ??
        NIC Model equivalent for id attribute : ??
    
    The bibinfo element is used to store all the metadata of a document.
  -->
  <xs:element name="bibinfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="contact"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="directorate"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="committee"/>
        <xs:element minOccurs="0" ref="cancel"/>
        <xs:element minOccurs="0" ref="destinat"/>
        <xs:element minOccurs="0" ref="docdates"/>
        <xs:element minOccurs="0" ref="langinfo"/>
        <xs:element minOccurs="0" ref="pubyear"/>
        <xs:element minOccurs="0" ref="serinfo"/>
        <xs:element minOccurs="0" ref="maintitl"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="subtitle"/>
        <xs:element minOccurs="0" ref="abbrtitl"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="originatingbody"/>
        <xs:element minOccurs="0" ref="disclmr"/>
        <xs:element minOccurs="0" ref="mtginfo"/>
        <xs:element minOccurs="0" ref="comment"/>
        <xs:element minOccurs="0" ref="jobno"/>
        <xs:element minOccurs="0" ref="traninfo"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="keywdgrp"/>
        <xs:element minOccurs="0" ref="dochist"/>
        <xs:element minOccurs="0" ref="abstract"/>
        <xs:element minOccurs="0" ref="action"/>
        <xs:element minOccurs="0" ref="isbn"/>
        <xs:element minOccurs="0" ref="doi"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.bibinfo"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.bibinfo">
    <xs:attributeGroup ref="id"/>
    <xs:attribute name="type">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="agenda"/>
          <xs:enumeration value="mtg-sum"/>
          <xs:enumeration value="narr-doc"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for biblio element : ??
        NIC Model equivalent for label attribute : ??
        NIC Model equivalent for number attribute : ??
        NIC Model equivalent for numbering attribute : ??
    
    Container used to store the bibliographical data. Converted form Word style Bibliography Heading.
  -->
  <xs:element name="biblio">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="heading"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="subheading"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="section"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="randlist"/>
        </xs:sequence>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="p"/>
          <xs:element ref="indvpub"/>
          <xs:element ref="instpub"/>
          <xs:element ref="oecd-doc"/>
          <xs:element ref="article"/>
          <xs:element ref="periodcl"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.biblio"/>
    </xs:complexType>
  </xs:element>
  <!--
    @attr label is used to store the numbering to be displayed.
    @attr number is used to store the numbering to be displayed.
    @attr numbering is used to activate the numbering. Possible values are <i>y</i> (yes) or <i>n</i> (no).
  -->
  <xs:attributeGroup name="attlist.biblio">
    <xs:attributeGroup ref="id"/>
    <xs:attribute name="label"/>
    <xs:attribute name="number"/>
    <xs:attribute name="numbering" default="n">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="y"/>
          <xs:enumeration value="n"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- Element not used -->
  <xs:element name="bibref">
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="attlist.bibref"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.bibref">
    <xs:attribute name="refid" use="required" type="xs:IDREF"/>
    <xs:attribute name="format"/>
    <xs:attribute name="capital" default="yes">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="yes"/>
          <xs:enumeration value="no"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for body element : ??
    
    The body element is used to store all the elements like parts and chapter of a document. Il can contains:<br>
    <ul><li>part</li>
    <li>chapter</li>
    <li>section</li></ul>
  -->
  <xs:element name="body">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="unbounded" ref="part"/>
        <xs:element maxOccurs="unbounded" ref="chapter"/>
