  </xs:element>
  <!--
    @attr type define the text to be displayed. The possible values are:<br>
    <ul><li>alpha for lower alpha ordered list</li>
    <li>number for arabic number ordered list</li>
    <li>note to display the word <i>note</i></li>
    <li>notes to display the word <i>notes</i></li>
    <li>.. to display ".." (two dots). Means nil or negligeable</li>
    <li>hyphen to display "-"</li>
    <li>e for estimation</li>
    <li>n.a. for not available</li>
    <li>n.c. for non communiqué (in french)</li>
    <li>n.d. for non disponible (in french)</li>
    <li>none to display nothing</li>
    <li>p for provisionnal</li>
    <li>ast for "*"</li></ul>
  -->
  <xs:attributeGroup name="attlist.fignote">
    <xs:attributeGroup ref="id"/>
    <xs:attribute name="type" default="number">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="alpha"/>
          <xs:enumeration value="number"/>
          <xs:enumeration value="note"/>
          <xs:enumeration value="notes"/>
          <xs:enumeration value=".."/>
          <xs:enumeration value="hyphen"/>
          <xs:enumeration value="e"/>
          <xs:enumeration value="n.a."/>
          <xs:enumeration value="n.c."/>
          <xs:enumeration value="n.d."/>
          <xs:enumeration value="none"/>
          <xs:enumeration value="p"/>
          <xs:enumeration value="ast"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for fignoteref element : ??
    
