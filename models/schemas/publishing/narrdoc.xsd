    <xs:attribute name="float" default="y">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="y"/>
          <xs:enumeration value="n"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="orient" default="port">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="land"/>
          <xs:enumeration value="port"/>
