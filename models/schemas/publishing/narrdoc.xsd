        <xs:element ref="emphasis"/>
        <xs:element ref="equation"/>
        <xs:element ref="specchar"/>
        <xs:element ref="indexword"/>
        <xs:element ref="tabnoteref"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.tablemainhead"/>
