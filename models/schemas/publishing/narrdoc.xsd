  <!--
    @attr font is the font to use. The possible values are:
        <ul><li>euro</li>
        <li>zapfdingbats</li>
        <li>symbol</li>
        <li>timesce</li>
        <li>timescyr</li>
        <li>default</li></ul>
    @attr position is the vertical position of the characters. The posssible values are:
        <ul><li>sup</li>
        <li>sub</li>
        <li>regular</li></ul>
  -->
  <xs:attributeGroup name="attlist.specchar">
    <xs:attribute name="font" default="default">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="euro"/>
          <xs:enumeration value="zapfdingbats"/>
          <xs:enumeration value="symbol"/>
          <xs:enumeration value="timesce"/>
          <xs:enumeration value="timescyr"/>
          <xs:enumeration value="default"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="position" default="regular">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="sup"/>
          <xs:enumeration value="sub"/>
          <xs:enumeration value="regular"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- Element not used -->
