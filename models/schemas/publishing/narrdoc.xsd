  <!--
        NIC Model equivalent for spanspec element : ??
        NIC Model equivalent for spanname attribute : ??
        NIC Model equivalent for namest attribute : ??
        NIC Model equivalent for nameend attribute : ??
        NIC Model equivalent for align attribute : ??
        NIC Model equivalent for colsep attribute : ??
        NIC Model equivalent for rowsep attribute : ??
    
    The span element defines each rowspan in the table and assigns it a name and which columns it spans.
  -->
  <xs:element name="spanspec">
