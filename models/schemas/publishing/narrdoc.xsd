  <!--
     DELETED 16-07-2010
    <!ELEMENT tablist EMPTY>
    <!ATTLIST tablist
    id ID #IMPLIED>
  -->
  <!--
        NIC Model equivalent for tabnote element : ??
        NIC Model equivalent for id attribute : ??
