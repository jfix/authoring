          <xs:enumeration value="indicator"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="geocontainer"/>
    <xs:attributeGroup ref="statisticalannex"/>
    <xs:attributeGroup ref="metadatatype"/>
