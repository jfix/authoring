  <!--
    @attr align is the horizontal alignment for the text inside the column. The possible values are:
    @attr cols is the number of columns in the tgroup.
    @attr colsep is used to indicate if a rule have to be placed to the right of a column.
    @attr rowsep is used to indicate if a rule have to be placed below a row.
    @attr charoff
    @attr char is the character to be used in horizontal text alignment inside the column, for example a decimal point “.” 
    @attr type the type of the values containing in the cell. Possible values are <i>txt</i> or <i>num</i>.
    @attr status is the status of the table. Possible values are <i>user</i>, <i>ok</li> or <i>check</i>.
    @attr label is used to store the numbering to be displayed.
    @attr number is used to store the numbering to be displayed.
    @attr numbering is used to activate the numbering. Possible values are <i>y</i> (yes) or <i>n</i> (no).
    @attr reference is used to store the numbering to be displayed.
  -->
  <xs:attributeGroup name="attlist.tgroup">
    <xs:attribute name="align" default="left">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="left"/>
          <xs:enumeration value="right"/>
          <xs:enumeration value="center"/>
          <xs:enumeration value="justify"/>
          <xs:enumeration value="char"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="cols" use="required"/>
    <xs:attribute name="colsep"/>
    <xs:attribute name="rowsep"/>
    <xs:attribute name="charoff" default="0"/>
    <xs:attribute name="char"/>
    <xs:attribute name="type" default="num">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="txt"/>
          <xs:enumeration value="num"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="status">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="user"/>
          <xs:enumeration value="ok"/>
          <xs:enumeration value="check"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="label"/>
    <xs:attribute name="number"/>
    <xs:attribute name="reference"/>
    <xs:attribute name="numbering" default="n">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="y"/>
          <xs:enumeration value="n"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
        NIC Model equivalent for thead element : ??
    
