<!--
    Inline element used to display the text of an internal reference. 
-->
<!ELEMENT xref EMPTY>
<!--
    @attr refid
    @attr capital
    @attr display
    @attr object is the text to be display. Possible values are:
        <ul><li>annex</li>
        <li>box</li>
        <li>chapter</li>
        <li>cote</li>
        <li>figure</li>
        <li>part</li>
        <li>section</li>
        <li>table</li>
        <li>mainhead</li>
    @attr type is to define if the word "source" is singular or plural. The possible values are:
    <ul><li>sing for singular</li>
    <li>plural</li></ul>
    @attr cont
    @attr format
    @attr srcfile
-->
<!ATTLIST xref
refid IDREF #REQUIRED
capital (no | yes) "no"
display CDATA #IMPLIED
object (annex | box | chapter | cote | figure | part | section | table | mainhead) "table"
type (plu | sing) "sing"
cont (none | and | to | no) "none"
format CDATA #IMPLIED
srcfile CDATA #IMPLIED>

