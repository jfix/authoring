<!-- Element not used -->
<!ELEMENT indvpub (name+, (pubtitle, secrespn?, edition?, no-vols?, location?, publishr?, date, work-id?, volno?, issueno?, chapno?, pgnos?, sertitle?, translat?, isbn?, issn?))>
<!ATTLIST indvpub
%id;>

<!-- Element not used -->
<!ELEMENT info ((heading?, subheading*), (p | citation | deflist | randlist | seqlist)+)>

<!-- Element not used -->
<!ELEMENT institut (#PCDATA)>

<!-- Element not used -->
<!ELEMENT instpub (institut, (pubtitle, secrespn?, edition?, no-vols?, location?, publishr?, date, work-id?, volno?, issueno?, chapno?, pgnos?, sertitle?, translat?, isbn?, issn?))>
<!ATTLIST instpub
%id;>

<!--
    Container used to begin a introduction section. It store the text converted from the Word style Introduction Heading.<br>
    introduction element can be used in the following sections:
        <ul><li>frontmatter</li>
        <li>part</li></ul>
-->
<!ELEMENT introduction ((heading?, subheading*), authorlist?, highlight?, abstract?, ((p | citation | deflist | randlist | seqlist)+ | figure | tablegrp | textbox)*, section*, chapconcl?)>

<!--
    Container used to store the ISBN data. 
-->
<!ELEMENT isbn (#PCDATA)>

<!--
    Container used to store the ISSN data. 
-->
<!ELEMENT issn (#PCDATA)>

<!-- Element not used -->
<!ELEMENT issueno (#PCDATA)>

<!-- Element not used -->
<!ELEMENT jobno EMPTY>
<!ATTLIST jobno
jobnumber CDATA #REQUIRED
oecdcode CDATA #REQUIRED>

<!-- Element not used -->
<!ELEMENT journal (#PCDATA)>

<!-- Element not used -->
<!ELEMENT keywdgrp (keyword+)>

<!-- Element not used -->
<!ELEMENT keyword (#PCDATA)>

<!--

    Container used to store the language of a document.
-->
<!ELEMENT langinfo EMPTY>
<!-- 
    @attr origlang define the original language of the document language. It can have the following values:
        <ul><li>o-eng for english</li>
        <li>o-fr for french</li>
        <li>o-de for deutsch</li>
        <li>o-bi-ling for bilingual</li>
        <li>o-other for other language</li></ul>
    @attr instlang define the language used in the document language. It can have the following values:
        <ul><li>i-eng for english</li>
        <li>i-fr for french</li>
        <li>i-de for deutsch</li>
        <li>i-bi-ling for bilingual</li>
        <li>i-other for other language</li></ul>
-->
<!ATTLIST langinfo
origlang (o-eng | o-fr | o-de | o-bi-ling | o-other) #IMPLIED
instlang (i-eng | i-fr | i-de | i-bi-ling | i-other) #REQUIRED>

<!-- Element not used -->
<!ELEMENT legend (deflist | p+)>

<!-- 
    Container used to store the text to be displayed in a list. Converted from possible Word style:
    <ul><li>List Bullet for a bulleted list</li>
    <li>List Number for a numbered list</li>
    <li>List Continue for a second entry in a list item</li>
    For the nested levels, the possible Word style are:
    <ul><li>List Bullet 2, for second level</li>
    <li>List Bullet 3, for third level</li>
    <li>List Number 2, for second level</li>
    <li>List Number 3, for third level</li>
    <li>List Continue 2, for second level</li>
    <li>List Continue 3, for third level</li></ul>
-->
<!ELEMENT li (p | citation | deflist | randlist | seqlist)+>
<!--
    @attr label is used to store the numbering to be displayed.
    @attr number is used to store the numbering to be displayed.
    @attr numbering is used to activate the numbering. Possible values are <i>y</i> (yes) or <i>n</i> (no).
-->
<!ATTLIST li
id ID #IMPLIED
label CDATA #IMPLIED
number CDATA #IMPLIED
numbering (y | n) "n">

<!-- Element not used -->
<!ELEMENT location (#PCDATA)>

<!--
    Container used to store the text of the titles to be displayed.
-->
<!ELEMENT mainhead (#PCDATA | bibref | boxnoteref | comment | emphasis | equation | indexword | note | noteref | shortcitation | specchar | xref | fnote | graphic | hyperlink)*>
<!ATTLIST mainhead
%id;>

<!-- Element not used -->
<!ELEMENT maintitl (#PCDATA)>
<!ATTLIST maintitl
%id;>

<!-- Element not used -->
<!ELEMENT mtginfo (p | citation | deflist | randlist | seqlist)+>

<!-- 
    Container used to store firstname and surname of an author.
-->
<!ELEMENT name (title?, firstname, surname)>

<!--
    The narr-doc element is the root element of the document.
-->
<!ELEMENT narr-doc (frontmatter?, body?, backmatter?)>
<!--
    @attr status is the status of the document. The possible values are draft or final.
    @attr notestyle is where the notes will be placed in the document. the possible values are:
    <ol><li>book to place the notes at the end of the document, in the backmatter element</li>
    <li>chapter to place the notes at the end of the chapter, in the chapter element</li>
    <li>page to place the notes at the bottom of the pages, in the element fnote</li>
    <li>part to place the note at the end of the part, in the element part</li></ol>
    @attr classification is in which category the document has to be classified. Possible values are:
    <ol><li>official</li>
    <li>publication</li>
    <li>confidential</li>
    <li>unclassified</li></ol>
    @attr sectioning is used to determined how the XML instance have to be sectioning:
    <ol><li><i>book</i> to keep the instance in one file</li>
    <li><i>chapter</i> to separate the instance by chapter</li>
    <li><i>part</li> to separate the instance by part</li></ol>
-->
<!ATTLIST narr-doc
status (draft | final) #IMPLIED
notestyle (book | chapter | page | part) "chapter"
classification (official | publication | confidential | unclassified) "publication"
sectioning (book | chapter | part) "chapter"
>

<!-- Element not used -->
<!ELEMENT no-vols (#PCDATA)>

<!-- 
    Container used to store the notes of the document.
-->
<!ELEMENT note (p | citation | deflist | randlist | seqlist | tablegrp)+>
<!ATTLIST note
%id;>

<!--
    Cross-reference of a note element.
-->
<!ELEMENT noteref EMPTY>
<!--
    @att refid is the reference to the corresponding note element. 
    @att format 
    @att srcfile
-->
<!ATTLIST noteref
refid IDREF #REQUIRED
format CDATA #IMPLIED
srcfile CDATA #IMPLIED>

<!-- Element not used -->
<!ELEMENT oecd-doc (maintitl?, pararef?)>
<!ATTLIST oecd-doc
%id;>

<!-- DELETED 16-07-2010
<!ELEMENT olisavlb EMPTY>
<!ATTLIST olisavlb
type (full | partial | unavail | complete) #REQUIRED>-->

<!-- DELETED 16-07-2010
<!ELEMENT olisdate EMPTY>-->

<!-- Element not used -->
<!ELEMENT originatingbody (p | citation | deflist | randlist | seqlist)+>
<!ATTLIST originatingbody 
type (secretar | chairman | delegatn | consult | extbody | secgen | other) "secretar">

<!-- 
    Container used to store the text of all the elements that are not heading. For exemple, p can be parent of li (list), entry (cell), ... 
-->
<!ELEMENT p (#PCDATA | bibref | boxnoteref | comment | emphasis | equation | fnote | graphic | hyperlink | indexword | note | noteref | shortcitation | specchar | tabnoteref | xref)*>
<!--
    @attr indent is used to indent the first line of a paragraph. Possible values are <i>y</i> (yes) or <i>n</i> (no).
    
    @attr label is used to store the numbering to be displayed.
    @attr number is used to store the numbering to be displayed.
    @attr numbering is used to activate the numbering. Possible values are <i>y</i> (yes) or <i>n</i> (no).
    @attr instlang define the language used in the document language. It can have the following values:
        <ul><li>i-eng for english</li>
        <li>i-fr for french</li>
        <li>i-de for deutsch</li>
        <li>i-bi-ling for bilingual</li>
        <li>i-other for other language</li></ul>
-->
<!ATTLIST p
%id;
indent (y | n) "y"
%instlang;
label CDATA #IMPLIED
number CDATA #IMPLIED
numbering (y | n) "n">

<!-- Element not used -->
<!ELEMENT pararef (#PCDATA)>

<!-- 
    Container used to begin a part section. Converted frow Word style Part Heading.
-->
<!ELEMENT part (bibinfo?, (heading?, subheading*), authorlist?, foreword?, acknowledgement?, highlight?, abstract?, introduction?, chapter+, conclusion?, glossary-acronyms?, biblio?, appendix*, annex*, info?)>
<!--
    @attr label is used to store the numbering to be displayed.
    @attr number is used to store the numbering to be displayed.
    @attr numbering is used to activate the numbering. Possible values are <i>y</i> (yes) or <i>n</i> (no).
    @attr mode
    @attr instlang define the document language. It can have the following values:
        <ul><li>i-eng for english</li>
        <li>i-fr for french</li>
        <li>i-de for deutsch</li>
        <li>i-bi-ling for bilingual</li>
        <li>i-other for other language</li></ul>
    @attr reference is used to store the numbering to be displayed.
    @attr geocontainer is requested for metadata loader. Possible values are <i>false</i> or <i>true</i>.
    @attr statisticalannex is requested for metadata loader. Possible values are <i>false</i> or <i>true</i>.
    @attr metadatatype is requested for metadata loader to re-define the part as a section. The value is <i>section</i>.
-->
<!ATTLIST part
%id;
label CDATA #IMPLIED
number CDATA #IMPLIED
numbering (y | n) "n"
mode (standard) "standard"
reference CDATA #IMPLIED
%instlang;
%geocontainer;
%statisticalannex;
%metadatatype;
>

<!-- Element not used -->
<!ELEMENT periodcl (journal, location, date, volno?, issueno?, pgnos?)>
<!ATTLIST periodcl
id ID #IMPLIED>

<!-- Element not used -->
<!ELEMENT pgnos (#PCDATA)>

<!--
    Container used to display the position of an author.
-->
<!ELEMENT position (#PCDATA)>

<!--

    Container used to begin a preface section.<br>
    preface element can be used in the frontmatter element
-->
<!ELEMENT preface ((heading?, subheading*), authorlist?, highlight?, abstract?, ((p | citation | deflist | randlist | seqlist)+ | figure | tablegrp | textbox)*, section*, chapconcl?)>
<!ATTLIST preface
%id;>

<!-- Element not used -->
<!ELEMENT publishr (#PCDATA)>

<!-- Element not used -->
<!ELEMENT pubtitle (#PCDATA)>

<!-- Element not used -->
<!ELEMENT pubyear (#PCDATA)>

<!-- Element not used -->
<!ELEMENT quote (p | citation | deflist | randlist | seqlist)+>

<!-- 
    Element used to define a random list (for exemple, bulleted list). 
-->
<!ELEMENT randlist (li+)>
<!-- 
    @attr type is used to define the type of the list. The possible values are:
    <ul><li>bullet for bulleted list corresponding to first level</li>
    <li>zapf for the list beginning with character in zapf font. Corresponding to second level</li> 
    <li>hyphen for hyphen list corresponding to third level</li>
-->
<!ATTLIST randlist
type (bullet | hyphen | zapf) "bullet">

<!-- 
    Container used for all the cells in a table row.
-->
<!ELEMENT row (entry+)>
<!--
    @attr rowsep indicates whether a rule be placed below a row.
-->
<!ATTLIST row
	rowsep CDATA #IMPLIED>

<!-- Element not used -->
<!ELEMENT secrespn (#PCDATA)>

<!-- 

    Container used to store the subtitles in chapter, for exemple.

-->
<!ELEMENT section (bibinfo?, (heading?, subheading*), highlight?, abstract?, ((p | citation | deflist | randlist | seqlist)+ | quote | figure | tablegrp | textbox | section | biblio)*)>
<!--
    @attr label is used to store the numbering to be displayed.

    @attr number is used to store the numbering to be displayed.

    @attr numbering is used to activate the numbering. Possible values are <i>y</i> (yes) or <i>n</i> (no).

    @attr content. Possible values are:
    <ul><li>default</li>
    <li>indicator</li></ul>
    @attr geocontainer is requested for metadata loader. Possible values are <i>false</i> or <i>true</i>.

    @attr statisticalannex is requested for metadata loader. Possible values are <i>false</i> or <i>true</i>.

    @attr metadatatype is requested for metadata loader to re-define the section as a section. The value is <i>section</i>.


-->
<!ATTLIST section
%id;
label CDATA #IMPLIED
number CDATA #IMPLIED
numbering (y | n) "n"
content (default | indicator) "default"
%geocontainer;
%statisticalannex;
%metadatatype;
>

<!--

    Element used to define a sequantial list (for exemple, numbered list). 
-->
<!ELEMENT seqlist (li+)>
<!-- 
    @attr type is used to define the type of the list. The possible values are:
    <ul><li>arabic for the numbered list</li>
    <li>lalpha for lower alpha list</li> 
    <li>ualpha for upper alpha list</li>
    <li>lroman for lower roman list</li> 
    <li>uroman for upper roman list</li>
    <li>hardcoded is the text to be displayed</li></ul>
-->
<!ATTLIST seqlist
type (arabic | lalpha | ualpha | lroman | uroman | hardcoded) "arabic">

<!-- Element not used -->
<!ELEMENT serinfo (sertitle, issueno?)>

<!-- Element not used -->
<!ELEMENT sertitle (#PCDATA)>

<!--

    container used to store the text to be displayed between quotes.
-->
<!ELEMENT shortcitation (#PCDATA | emphasis | specchar | noteref | note | indexword | shortcitation)*>
<!ATTLIST shortcitation
citationsource CDATA #IMPLIED>

<!-- DELETED 16-07-2010
<!ELEMENT size EMPTY>
<!ATTLIST size
size CDATA #IMPLIED>-->

<!-- 
    Container used to store the text of the source for the box, figure and table. 
-->
<!ELEMENT source (p | citation | deflist | randlist | seqlist)+>
<!-- 
    @attr type is to define if the word "source" is singular or plural. The possible values are:
    <ul><li>sing for singular</li>
    <li>plural</li></ul>
-->
<!ATTLIST source
type (sing | plu) "sing">

<!--
    The span element defines each rowspan in the table and assigns it a name and which columns it spans.
-->
<!ELEMENT spanspec EMPTY>
<!-- 
    @attr spanname is the name of the span.
    @attr namest is the column number where the span start.
    @attr nameend is the column number where the span end.
    @attr align is the alignment in the cell. Possible values are:
        <ul><li>left</li>
        <li>right</li>
        <li>center</li>
        <li>justify</li>
        <li>char</li></ul>
    @attr charoff
    @attr char is the character to be used in horizontal text alignment inside the column, for example a decimal point “.” 
    @attr colsep is used to indicate if a rule have to be placed to the right of a column.
    @attr rowsep is used to indicate if a rule have to be placed below a row.
-->
<!ATTLIST spanspec
spanname CDATA #REQUIRED
namest CDATA #REQUIRED
nameend CDATA #REQUIRED
align (left | right | center | justify | char) "center"
charoff CDATA #IMPLIED
char CDATA #IMPLIED
colsep CDATA #IMPLIED
rowsep CDATA #IMPLIED
>

<!--

    Inline element used to change the display of the text within specchar tag. 
-->
<!ELEMENT specchar (#PCDATA)>

<!--
    @attr font is the font to use. The possible values are:
        <ul><li>euro</li>
        <li>zapfdingbats</li>
        <li>symbol</li>
        <li>timesce</li>
        <li>timescyr</li>
        <li>default</li></ul>
    @attr position is the vertical position of the characters. The posssible values are:
        <ul><li>sup</li>
        <li>sub</li>
        <li>regular</li></ul>
-->
<!ATTLIST specchar
font (euro | zapfdingbats | symbol | timesce | timescyr | default) "default"
position (sup | sub | regular) "regular">

<!-- Element not used -->
<!ELEMENT strtdate (#PCDATA)>

<!--

    Container used to store the text of the different heading element listed above.
-->
<!ELEMENT subheading (mainhead, tochead?)>

<!-- Element not used -->
<!ELEMENT subtitle (#PCDATA)>

<!-- 
    Container used to display the surname of an author.
-->
<!ELEMENT surname (#PCDATA)>

<!--
    Element that identifies a table.
-->
<!ELEMENT table (tgroup+)>
<!-- 
    @attr frame describes position of outer rulings. Possible values are:
        <ul><li>top</li>
        <li>bottom</li>
        <li>topbot</li>
        <li>all</li>
        <li>sides</li>
        <li>none</li></ul>
    @attr colsep is the default for all tgroup in this table.
    @attr rowsep is the default for all tgroup in this table.
    @attr orient Orientation of the entire table.
    @attr pgwide
-->
<!ATTLIST table
frame (top | bottom | topbot | all | sides | none) #IMPLIED
colsep CDATA #IMPLIED
rowsep CDATA #IMPLIED
orient (port | land) #IMPLIED
pgwide CDATA #IMPLIED
>

<!--
    Container used to store the heading and subheading of a table.
-->
<!ELEMENT tablegrp (tableheading?, tablesubheading*, (table | graphic)*, (hyperlink | tabnote | source)*)>
<!--
    @attr instlang define the document language. It can have the following values:
        <ul><li>i-eng for english</li>
        <li>i-fr for french</li>
        <li>i-de for deutsch</li>
        <li>i-bi-ling for bilingual</li>
        <li>i-other for other language</li></ul>
    @attr orient Orientation of the entire table.
    @attr label is used to store the numbering to be displayed.
    @attr reference is used to store the numbering to be displayed.
    @attr name
    @attr number is used to store the numbering to be displayed.
    @attr numbering is used to activate the numbering. Possible values are <i>y</i> (yes) or <i>n</i> (no).
    @attr type is the type of the table. Possible values are <i>table</i> of <i>exhibit</i>.
    @attr dynamic-graph
-->
<!ATTLIST tablegrp
%id;
%instlang;
orient (land | port) "port"
label CDATA #IMPLIED
name CDATA #IMPLIED
number CDATA #IMPLIED
numbering (y | n) "y"
type (table | exhibit) "table"
reference CDATA #IMPLIED
dynamic-graph (y | n) #IMPLIED>

<!--
    container used to store the heading of a table.
-->
<!ELEMENT tableheading (tablemainhead, tochead?)>

<!-- 
    Container used to display the text of a table heading or table subheading.
-->
<!ELEMENT tablemainhead (#PCDATA | shortcitation | comment | emphasis | equation | specchar | indexword | tabnoteref)*>
<!ATTLIST tablemainhead 
%id;>

<!--
    container used to store the subheading of a table.
-->
<!ELEMENT tablesubheading (tablemainhead, tochead?)>

<!-- DELETED 16-07-2010
<!ELEMENT tablist EMPTY>
<!ATTLIST tablist
id ID #IMPLIED>-->

<!--
   Container used to store the notes of a table. Converted from the Word style Table Note.
-->
<!ELEMENT tabnote (p | citation | deflist | randlist | seqlist)+>
<!--
    @attr type define the text to be displayed. The possible values are:<br>
    <ul><li>alpha for lower alpha ordered list</li>
    <li>number for arabic number ordered list</li>
    <li>note to display the word <i>note</i></li>
    <li>notes to display the word <i>notes</i></li>
    <li>.. to display ".." (two dots). Means nil or negligeable</li>
    <li>hyphen to display "-"</li>
    <li>e for estimation</li>
    <li>n.a. for not available</li>
    <li>n.c. for non communiqué (in french)</li>
    <li>n.d. for non disponible (in french)</li>
    <li>none to display nothing</li>
    <li>p for provisionnal</li>
    <li>ast for "*"</li></ul>
-->
<!ATTLIST tabnote
%id;
type (alpha | number | note | notes | .. | hyphen | e | n.a. | n.c. | n.d. | none | p | ast) "number">

<!--
    Cross-reference of a tabnote element.
-->
<!ELEMENT tabnoteref EMPTY>
<!--
    @att refid is the reference to the corresponding tabnote element. 
    @att format 
    @att srcfile
-->
<!ATTLIST tabnoteref 
refid IDREF #REQUIRED
format CDATA #IMPLIED
srcfile CDATA #IMPLIED>

<!--
    Element that identifies the body of a tgroup. 
-->
<!ELEMENT tbody (row+)>
<!--
    @attr valign is the default text vertical positioning within the entry. Possible values are:
        <ul><li>top</li>
        <li>middle</li>
        <li>bottom</li></ul>
-->
<!ATTLIST tbody
valign (top | middle | bottom) "top">

<!--
    Container used to display the telephone of an author.
-->
<!ELEMENT telephone (#PCDATA)>

<!-- 
    Container used to store the term of a glossary.
-->
<!ELEMENT term (p | citation | deflist | randlist | seqlist)+>

<!-- 
   Container used to begin a box section. It store the text converted from the Word style box Heading.<br>
-->
<!ELEMENT textbox (boxsection)>
<!ATTLIST textbox
%id;>

<!--
    Each tgroup effectively identifies a new portion of a table.
-->
<!ELEMENT tgroup (colspec*, spanspec*, thead?, tbody)>
<!--
    @attr align is the horizontal alignment for the text inside the column. The possible values are:
    @attr cols is the number of columns in the tgroup.
    @attr colsep is used to indicate if a rule have to be placed to the right of a column.
    @attr rowsep is used to indicate if a rule have to be placed below a row.
    @attr charoff
    @attr char is the character to be used in horizontal text alignment inside the column, for example a decimal point “.” 
    @attr type the type of the values containing in the cell. Possible values are <i>txt</i> or <i>num</i>.
    @attr status is the status of the table. Possible values are <i>user</i>, <i>ok</li> or <i>check</i>.
    @attr label is used to store the numbering to be displayed.
    @attr number is used to store the numbering to be displayed.
    @attr numbering is used to activate the numbering. Possible values are <i>y</i> (yes) or <i>n</i> (no).
    @attr reference is used to store the numbering to be displayed.
-->
<!ATTLIST tgroup
align (left | right | center | justify | char) "left"
cols CDATA #REQUIRED
colsep CDATA #IMPLIED
rowsep CDATA #IMPLIED
charoff CDATA "0"
char CDATA #IMPLIED
type (txt | num) "num"
status (user | ok | check) #IMPLIED
label CDATA #IMPLIED
number CDATA #IMPLIED
reference CDATA #IMPLIED
numbering (y | n) "n">

<!--
    Container that identifies the heading row of a tgroup. 
-->
<!ELEMENT thead (colspec*, row+)>
<!--
    @attr valign is the default text vertical positioning within the entry.
-->
<!ATTLIST thead
valign (top | middle | bottom) #IMPLIED>

