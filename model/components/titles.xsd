<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    
    <xs:annotation>
        <xs:documentation><section xmlns="http://docbook.org/ns/docbook">
            <title>OECD Schemas</title>
            <subtitle>Title type components</subtitle>
            <revhistory>
                <title>Change Log</title>
                
                
                    <revision>
                        <revnumber>0.1</revnumber>
                        <date>2012-02-10</date>
                        <author><email>nicg@corbas.co.uk</email><personname>Nic Gibson</personname></author>
                        <revremark>Initial Version</revremark>
                    </revision>
                    <revision>
                        <revnumber>0.2</revnumber>
                        <date>2012-03-14</date>
                        <author><email>nicg@corbas.co.uk</email><personname>Nic Gibson</personname></author>
                        <revremark>Rewrite to use the more structure group/attributeGroup approach used in other components.</revremark>
                        
                    </revision>
                
            </revhistory>
            
        </section></xs:documentation>
    </xs:annotation>
    
    
    <!-- IMPORTS AND INCLUDES -->
    <xs:include schemaLocation="attributes.xsd"/>
    <xs:include schemaLocation="inlines.xsd"/>
    
    
    <!-- GLOBAL ELEMENTS -->
    <!-- TODO: determine if these really all should be the same type -->
    <xs:element name="title" type="oecdTitleType"/>
    <xs:element name="subTitle" type="oecdTitleType"/>
    <xs:element name="shortTitle" type="oecdShortTitleType"/>
    <xs:element name="caption" type="oecdTitleType"/>
    
    <!-- GLOBAL GROUPS -->
    <xs:group name="oecdTitleGroup">
        <xs:sequence>
            <xs:element ref="title"/>
            <xs:element ref="subTitle" minOccurs="0"/>
            <xs:element ref="shortTitle"/>
        </xs:sequence>
    </xs:group>
    
    <!-- GLOBAL TYPES -->
    <xs:attributeGroup name="oecdTitleTypeAttributes">
        <xs:annotation>
            <xs:documentation>
                <section xmlns="http://docbook.org/ns/docbook">
                    <para>Attribute group for title type elements to allow easy overrides.</para>
                </section>
            </xs:documentation>
        </xs:annotation>
        <xs:attributeGroup ref="oecdCommonAttributes"/>
        <xs:attributeGroup ref="oecdLinkingAttributes"/>
    </xs:attributeGroup>
    
    <xs:group name="oecdTitleTypeContent">
        <xs:annotation>
            <xs:documentation>
                <section xmlns="http://docbook.org/ns/docbook">
                    <para>Content for title type elements to allow easy overrides.</para>
                </section>
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:group ref="oecdInlineGroup" maxOccurs="unbounded" minOccurs="0"/>
        </xs:sequence>
    </xs:group>
    
    <xs:complexType name="oecdTitleType" mixed="true">
        <xs:annotation>
            <xs:documentation>
                <section xmlns="http://docbook.org/ns/docbook">
                    <para>Title elements (and subtitles) share this type.</para>
                </section>
            </xs:documentation>
        </xs:annotation>       
        <xs:group ref="oecdTitleTypeContent"/>
        <xs:attributeGroup ref="oecdTitleTypeAttributes"/>
    </xs:complexType>
    
    <!-- abbreviated title -->
    <xs:attributeGroup name="oecdShortTitleTypeAttributes">
        <xs:annotation>
            <xs:documentation>
                <section xmlns="http://docbook.org/ns/docbook">
                    <para>Attribute group for short title type elements to allow easy overrides.</para>
                </section>
            </xs:documentation>
        </xs:annotation>
        <xs:attributeGroup ref="oecdCommonAttributes"/>
        <xs:attributeGroup ref="oecdLinkingAttributes"/>
    </xs:attributeGroup>
    
    <xs:complexType name="oecdShortTitleType">
        <xs:annotation>
            <xs:documentation>
                <section xmlns="http://docbook.org/ns/docbook">
                    <para>Short title type. This element holds a text only abbreviated title</para>
                </section>
            </xs:documentation>
        </xs:annotation>
        
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attributeGroup ref="oecdShortTitleTypeAttributes"/>
            </xs:extension>
        </xs:simpleContent>
 
    </xs:complexType>
    
    
    <!-- SIMPLE TYPES -->
    
    
</xs:schema>