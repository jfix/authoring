			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Superscript type attributes grouped for easy override and reuse.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
	</xs:attributeGroup>
	<xs:complexType id="oecdSuperscriptType" mixed="true" name="oecdSuperscriptType">
		<xs:annotation>
			<xs:documentation>
				<div xmlns="http://docbook.org/ns/docbook">
					<revision>
						<revnumber>4</revnumber>
						<date>2012-04-15</date>
						<authorinitials>NG</authorinitials>
						<revremark>Initial version</revremark>
					</revision>
					<para>Simple superscripted text type</para>
				</div>
			</xs:documentation>
