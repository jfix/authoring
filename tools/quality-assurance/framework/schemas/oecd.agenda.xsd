	<xs:import namespace="http://www.loc.gov/mods/v3" schemaLocation="mods.xsd"/>
	<xs:simpleType id="oecdCharacterStyleType" name="oecdCharacterStyleType">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Simple enumeration of the inline character styles we might see. This is
						more intended as a demonstration than a model requirement. </para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="bold"/>
			<xs:enumeration value="italic"/>
			<xs:enumeration value="underline"/>
			<xs:enumeration value="strikethrough"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType id="oecdCote" name="oecdCote">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Placeholder for Cotes.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType id="oecdMediaType" name="oecdMediaType">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Placeholder for IETF MIME media types.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType id="oecdListMarker" name="oecdListMarker">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Bulleted lists.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="auto"/>
			<xs:enumeration value="square"/>
			<xs:enumeration value="solid-square"/>
			<xs:enumeration value="circle"/>
			<xs:enumeration value="solid-circle"/>
			<xs:enumeration value="diamonad"/>
			<xs:enumeration value="solid-diamonad"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType id="oecdListNumeration" name="oecdListNumeration">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>This defines numeration styles for numbered lists.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="arabic"/>
			<xs:enumeration value="lower-roman"/>
			<xs:enumeration value="upper-roman"/>
			<xs:enumeration value="lower-alpha"/>
			<xs:enumeration value="upper-alpha"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType id="oecdComponentContinuation" name="oecdComponentContinuation">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<revhistory>
						<title>Change Log</title>
						<revision>
							<revnumber>1</revnumber>
							<date>2011-11-06</date>
							<author>JC</author>
							<revremark>Generisize simpleType</revremark>
						</revision>
					</revhistory>
				</section>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Defines continuation options allowed on document components.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="continue"/>
			<xs:enumeration value="start"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:annotation>
