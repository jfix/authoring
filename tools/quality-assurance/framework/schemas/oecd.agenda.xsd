	<!-- Groups -->
	<xs:attributeGroup id="oecdXmlAttributes" name="oecdXmlAttributes">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Attribute group to grandfather in the XML Special Attributes.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="xml:specialAttrs"/>
	</xs:attributeGroup>
	<xs:attributeGroup id="oecdIdRequiredXmlAttributes" name="oecdIdRequiredXmlAttributes">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Attribute group to grandfather in the XML Special Attributes. This variant
						differs in that the xml:id attribute is required.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:base"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute ref="xml:space"/>
		<xs:attribute ref="xml:id" use="required"/>
	</xs:attributeGroup>
	<xs:attributeGroup id="oecdLinkingAttributes" name="oecdLinkingAttributes">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Attribute group to grandfather in the XLink Attribute Set.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="xlink:simpleAttrs"/>
		<xs:attributeGroup ref="XREF-GROUP"/>
	</xs:attributeGroup>
	<xs:attributeGroup id="oecdRequiredLinkingAttributes" name="oecdRequiredLinkingAttributes">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Attribute group to grandfather in the XLink Attribute Set with a required
						external link.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xlink:href" use="required"/>
		<xs:attribute form="qualified" id="type" name="type" type="xs:string"/>
		<xs:attribute ref="xlink:role"/>
		<xs:attribute ref="xlink:arcrole"/>
		<xs:attribute ref="xlink:title"/>
		<xs:attribute ref="xlink:show"/>
		<xs:attribute ref="xlink:actuate"/>
	</xs:attributeGroup>
	<xs:attributeGroup id="oecdCommonAttributes" name="oecdCommonAttributes">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<revision>
						<revnumber>2</revnumber>
						<date>2011-12-29</date>
						<author>
							<email>nicg@corbas.co.uk</email>
							<personname>Nic Gibson</personname>
						</author>
						<revremark>Added <link linkend="label">label</link> attribute.</revremark>
					</revision>
					<revision>
						<revnumber>3</revnumber>
