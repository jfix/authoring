	<!-- GLOBAL ELEMENTS -->
	<xs:element id="metadataItem" name="metadata-item" type="oecdMetadataItemType"/>
	<xs:element id="metadata" name="metadata" type="oecdMetadataType"/>
	<!-- GLOBAL ATTRIBUTE -->
	<!-- N.B. The metadataRef attribute is defined in attributes.xsd -->
	<!-- TYPES -->
	<!-- metadata group type -->
	<xs:attributeGroup id="oecdMetadataTypeAttributes" name="oecdMetadataTypeAttributes">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<revhistory>
						<title>Change Log</title>
						<revision>
							<revnumber>2</revnumber>
							<date>2012-08-06</date>
							<authorinitials>NG</authorinitials>
							<revremark>Added new attributes - subject and subjectURI to flesh out
								metadata model.</revremark>
						</revision>
						<revision>
							<revnumber>3</revnumber>
							<date>2012-10-05</date>
							<authorinitials>NG</authorinitials>
							<revremark>Moved element definitions into the group.</revremark>
						</revision>
						<revision>
							<revnumber>4</revnumber>
							<date>2012-10-26</date>
							<authorinitials>NG</authorinitials>
							<revremark>Modified to include the property attributes in order to
								satisfy <link
									xlink:href="https://pacps01.oecd.org/redmine/issues/4872">issue
									4872</link>.</revremark>
						</revision>
					</revhistory>
					<para>Attributes for metadata. Grouped to allow easy override.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="oecdCommonAttributes"/>
		<xs:attributeGroup ref="oecdMetaSubjectAttributes"/>
		<xs:attributeGroup ref="oecdMetaPropertyAttributes"/>
	</xs:attributeGroup>
	<xs:group id="oecdMetadataTypeContent" name="oecdMetadataTypeContent">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<revhistory>
						<revision>
							<revnumber>2</revnumber>
							<date>2012-08-06</date>
							<authorinitials>NG</authorinitials>
							<revremark>Major rewrite. Rebuilt content model to require one or other
								of metadataItem or metadata plus zero or more of either. We require
								that grouped metadata follows individual items simply to make the
								schema sane (and non-ambigous).</revremark>
						</revision>
						<revision>
							<revnumber>2</revnumber>
							<date>2012-11-08</date>
							<author>JC</author>
							<revremark>Modified element name for to use the lower-case convention
								[#4947].</revremark>
						</revision>
					</revhistory>
					<para>Content for metadata. Grouped to allow easy override.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" ref="metadata-item"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" ref="metadata"/>
		</xs:sequence>
	</xs:group>
	<xs:complexType id="oecdMetadataType" name="oecdMetadataType">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Metadata container element. Allowed as a child at almost any location in a
						document.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:group maxOccurs="unbounded" ref="oecdMetadataTypeContent"/>
		<xs:attributeGroup ref="oecdMetadataTypeAttributes"/>
	</xs:complexType>
	<!-- metdata item type. -->
	<xs:attributeGroup id="oecdMetadataItemTypeAttributes" name="oecdMetadataItemTypeAttributes">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<revhistory>
						<title>Change Log</title>
						<revision>
							<revnumber>2</revnumber>
							<date>2012-08-06</date>
							<authorinitials>NG</authorinitials>
							<revremark>Added new attributes - subject, property, value - and their
								URI based alternatives to flesh out metadata model.</revremark>
						</revision>
					</revhistory>
					<para>Attributes for metadata items. Grouped to allow easy override.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="oecdCommonAttributes"/>
		<xs:attributeGroup ref="oecdLinkingAttributes"/>
		<xs:attributeGroup ref="oecdMetaSubjectAttributes"/>
		<xs:attributeGroup ref="oecdMetaPropertyAttributes"/>
		<xs:attributeGroup ref="oecdMetaValueAttributes"/>
	</xs:attributeGroup>
	<xs:group id="oecdMetadateItemTypeContent" name="oecdMetadateItemTypeContent">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Content for metadata items. Grouped to allow easy override.</para>
					<para>The content model for metadata is currently defined as empty as all value
						is carried on the attributes.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence/>
	</xs:group>
	<xs:complexType id="oecdMetadataItemType" name="oecdMetadataItemType">
		<xs:annotation>
			<xs:documentation xmlns:html="http://www.w3.org/1999/xhtml">
				<section xmlns="http://docbook.org/ns/docbook">
					<para>The content model for metadata is currently defined as a single element
						from any namespace. This is unlikely to be the final model and simply acts
						as a placeholder.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:group ref="oecdMetadateItemTypeContent"/>
		<xs:attributeGroup ref="oecdMetadataItemTypeAttributes"/>
	</xs:complexType>
	<!-- SIMPLE TYPES -->
	<xs:simpleType name="oecdStringMetaType">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<revhistory>
						<revnumber>2</revnumber>
						<date>2012-08-06</date>
						<authorinitials>NG</authorinitials>
						<revremark>Initial version</revremark>
					</revhistory>
					<para>The OECD string meta type is defined in order to allow a shared textual
						data type for string based metadata so that all three attributes below have
						a common type (non-empty string).</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="oecdURIMetaType">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<revhistory>
						<revnumber>2</revnumber>
						<date>2012-08-06</date>
						<authorinitials>NG</authorinitials>
						<revremark>Initial version</revremark>
					</revhistory>
					<para>The OECD URI meta type is defined in order to allow a shared data type for
						URI based metadata so that all three attributes below have a common type
						(non-blank anyURI).</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ATTRIBUTES -->
	<xs:attributeGroup id="oecdMetaSubjectAttributes" name="oecdMetaSubjectAttributes">
		<xs:annotation>
			<xs:documentation>
				<para xmlns="http://docbook.org/ns/docbook">Wrap up metadata subject
					attributes.</para>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="subject" type="oecdStringMetaType">
			<xs:annotation>
				<xs:documentation>
					<section xmlns="http://docbook.org/ns/docbook">
						<para>Attribute used to indicate the subject of a metadata statement. This
							element can be omitted to indicate that it refers to the enclosing or
							referring element or if it is part of a group of metadata items with a
							specified subject.</para>
					</section>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="subject-uri" type="oecdURIMetaType">
			<xs:annotation>
				<xs:documentation>
					<section xmlns="http://docbook.org/ns/docbook">
						<para>Attribute used to indicate the subject of a metadata statement.
							Identical to subject above but requires a URI.</para>
					</section>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup id="oecdMetaPropertyAttributes" name="oecdMetaPropertyAttributes">
		<xs:annotation>
			<xs:documentation>
				<para xmlns="http://docbook.org/ns/docbook">Wrap up metadata property
					attributes.</para>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="property" type="oecdStringMetaType">
			<xs:annotation>
				<xs:documentation>
					<section xmlns="http://docbook.org/ns/docbook">
						<para>Attribute used to indicate the property of an entity in a metadata
							statement.</para>
					</section>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="property-uri" type="oecdURIMetaType">
			<xs:annotation>
				<xs:documentation>
					<section xmlns="http://docbook.org/ns/docbook">
						<para>Attribute used to indicate the property of an entity in a metadata
							statement. Identical to property above but requires a URI.</para>
					</section>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup id="oecdMetaValueAttributes" name="oecdMetaValueAttributes">
		<xs:annotation>
			<xs:documentation>
				<para xmlns="http://docbook.org/ns/docbook">Wrap up metadata value
					attributes.</para>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="value" type="oecdStringMetaType">
			<xs:annotation>
				<xs:documentation>
					<section xmlns="http://docbook.org/ns/docbook">
						<para>Attribute used to indicate the value of an entity in a metadata
							statement.</para>
					</section>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="value-uri" type="oecdURIMetaType">
			<xs:annotation>
				<xs:documentation>
					<section xmlns="http://docbook.org/ns/docbook">
						<para>Attribute used to indicate the value of an entity in a metadata
							statement. Identical to value above but requires a URI.</para>
					</section>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<!-- GLOBAL ELEMENTS -->
	<xs:element abstract="true" id="oecdInline" name="oecd-inline" type="oecdInlineType"/>
	<!-- different name to all other elements as it is abstract -->
	<xs:element id="emphasis" name="emphasis" substitutionGroup="oecd-inline"
		type="oecdEmphasisType"/>
	<xs:element id="footnote" name="footnote" type="oecdFootnoteType"/>
	<xs:element id="text" name="text" type="oecdTextType"/>
	<xs:element id="sub" name="sub" substitutionGroup="oecd-inline" type="oecdSubscriptType"/>
	<xs:element id="sup" name="sup" substitutionGroup="oecd-inline" type="oecdSuperscriptType"/>
	<xs:element id="quote" name="quote" substitutionGroup="oecd-inline" type="oecdQuoteType"/>
	<xs:element id="phrase" name="phrase" substitutionGroup="oecd-inline" type="oecdPhraseType"/>
	<xs:element id="abbrev" name="abbrev" substitutionGroup="oecd-inline" type="oecdAbbrevitionType"/>
	<!-- COMPLEX TYPES -->
	<!-- abstract inline elements -->
	<xs:attributeGroup id="oecdInlineTypeAttributes" name="oecdInlineTypeAttributes">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Inline element base attributes grouped for easy override and reuse.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="oecdCommonAttributes"/>
		<xs:attributeGroup ref="oecdLinkingAttributes"/>
	</xs:attributeGroup>
	<xs:group id="oecdInlineTypeContent" name="oecdInlineTypeContent">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Inline element content grouped for easy override and reuse.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" ref="oecd-inline"/>
		</xs:sequence>
	</xs:group>
	<xs:complexType abstract="true" id="oecdInlineType" mixed="true" name="oecdInlineType">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>The oecdInline type is the base class of almost all inline components. The
						components may only contain text or other elements in general. The purpose
						of the type                         is to ensure that all the inlines share
						a type.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:group maxOccurs="unbounded" minOccurs="0" ref="oecdInlineTypeContent"/>
		<xs:attributeGroup ref="oecdInlineTypeAttributes"/>
	</xs:complexType>
	<!-- emphasis -->
	<xs:attributeGroup id="oecdEmphasisTypeAttributes" name="oecdEmphasisTypeAttributes">
		<xs:annotation>
			<xs:documentation>
				<section xmlns="http://docbook.org/ns/docbook">
					<para>Emphasis type attributes grouped for easy override and reuse.</para>
				</section>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute id="style" name="style" type="oecdStyleAttributeType" use="required"/>
	</xs:attributeGroup>
	<xs:complexType id="oecdEmphasisType" mixed="true" name="oecdEmphasisType">
